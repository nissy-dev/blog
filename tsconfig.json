{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "allowJs": true,
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react",
    "baseUrl": "src",
    "paths": {
      "markdown-toc": [
        "types/markdown-toc.d.ts.ts"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
